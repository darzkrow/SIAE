    "components": {
        "schemas": {
            "Accessory": {
                "type": "object",
                "description": "Serializer para accesorios.",
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": true
                    },
                    "sku": {
                        "type": "string",
                        "readOnly": true,
                        "description": "Código único de producto (se genera automáticamente)"
                    },
                    "nombre": {
                        "type": "string",
                        "maxLength": 250
                    },
                    "descripcion": {
                        "type": "string"
                    },
                    "categoria": {
                        "type": "integer"
                    },
                    "categoria_detail": {
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/Category"
                            }
                        ],
                        "readOnly": true
                    },
                    "categoria_nombre": {
                        "type": "string",
                        "readOnly": true
                    },
                    "unidad_medida": {
                        "type": "integer"
                    },
                    "unidad_medida_detail": {
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/UnitOfMeasure"
                            }
                        ],
                        "readOnly": true
                    },
